{
    "name": "eseperio/yii2-whatsapp-web-rest-client",
    "description": "A Yii2 client to handle connection with WhatsApp Web REST API (avoylenko/wwebjs-api)",
    "type": "yii2-extension",
    "keywords": ["yii2", "whatsapp", "whatsapp-web", "rest-api", "client", "wwebjs"],
    "license": "MIT",
    "authors": [
        {
            "name": "E.Alamo",
            "email": "eseperio@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-httpclient": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "codeception/verify": "~0.5.0 || ~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "eseperio\\whatsapp\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}